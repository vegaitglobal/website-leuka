@using Leuka.Core.ViewModels.Pages;
@using Leuka.Core.ViewModels.Shared;
@using Leuka.Core.ViewModels;
@inherits UmbracoViewPage<JoinUsViewModel>

@foreach (IContentViewModel component in Model.Components)
{
    if (component.GetType() == typeof(ContactusViewModel))
    {
        if (component is ContactusViewModel contactusViewModel && contactusViewModel.Enabled)
        {
            Html.RenderPartial(component.PartialViewPath, component);
        }
    }
    else
    {
        Html.RenderPartial(component.PartialViewPath, component);
    }
}