@inherits UmbracoViewPage<ArticleViewModel>

@if (Model.HeadlineInfo != null)
{
    Html.RenderPartial(Model.HeadlineInfo.PartialViewPath, Model.HeadlineInfo);
}

@if (Model.Breadcrumbs.Chain != null)
{
    Html.RenderPartial(Model.Breadcrumbs.PartialViewPath, Model.Breadcrumbs);
}

@Model.PageDetails
@if (Model.AuthorDetails != null)
{
    Html.RenderPartial(Model.AuthorDetails.PartialViewPath, Model.AuthorDetails);
}
